
@{
    ViewBag.Title = "Ocacvs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="body">

    <!-- start: header -->
    <!-- end: header -->
    <!-- start: sidebar -->
    <!-- end: sidebar -->

    <section role="main" class="">


        <!-- start: page -->


        <section class="panel">
            <header class="panel-heading">
                <div class="panel-actions">
                    <a href="/Admin/#" class="fa fa-caret-down"></a>

                </div>

                <h3 style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;color:black;">Organizations CA/ACCA CVs Submitted</h3>
            </header>
            <div class="panel-body">
                <table class="js-dynamitable table table-bordered table-striped mb-none" id="datatable-tabletools">

                    <!-- table heading -->
                    <thead>

                        <tr>
                            <th>Serial #</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>City</th>
                            <th>Age</th>
                            <th>ReferBy</th>
                            <th>Qualification</th>
                            <th>Status</th>
                            <th>Education Board</th>
                            <th>Date Of Qualification / No. Of Papers Left</th>
                            <th>Experience</th>
                            <th>Experience Time</th>
                            <th>Organization Name</th>
                            <th style="width:400px;">Actions<span style="color:#d41e48;">..........................</span></th>
                        </tr>
                        <tr id="sorting">
                            <th><input class="js-filter  form-control" type="text" value="" style="width:80px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="date" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>  
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th><input class="js-filter  form-control" type="text" value="" style="width:100px;"></th>
                            <th></th>

                        </tr>
                    </thead>

                    <!-- table body -->
                    <tbody>
                        @{
                            var count = 0;

                            foreach (var item in ViewBag.ocacv)
                            {
                                if (item.QName != null && item.QStatus != null)
                                {
                                    count = count + 1;
                                    <tr class="odd gradeX">
                                        <td>@count</td>
                                        <td>@item.Name</td>
                                        <td>@item.Email</td>
                                        <td>@item.City</td>
                                        <td>@item.DOB</td>
                                        <td>@item.ReferBy</td>
                                        <td>@item.QName</td>
                                        <td>@item.QStatus</td>
                                        <td>@item.EducationBoard</td>
                                        @if (item.QStatus == "Yes")
                                        {
                                            <td>@item.DOQ</td>
                                        }
                                        else
                                        {
                                            <td>@item.NOPL</td>
                                        }
                                        <td>@item.ExpStatus</td>
                                        <td>@item.NOYMExperience</td>
                                        <td>@item.OrgName</td>
                                        <td>
                                            <a href="@Url.Action("EditOcacvs","Home", new { id=item.Id })" class="btn btn-primary">Edit</a> |
                                            <a href="@Url.Action("DeleteOcacvs","Home", new { id=item.Id })" class="btn btn-danger">Delete</a>
                                        </td>
                                        @*<td><a class="btn btn-default" title="Download CV" href="@Url.Action("DownloadFile", "Orginfo", new { item.Id })"><i class="fa fa-download"></i></a></td>*@
                                    </tr>
                                }

                            }
                        }
                    </tbody>
                </table>


            </div>
            <!-- /.panel-body -->
        </section>
    </section>
</section>


<script src="~/scripts/jquery-3.3.1.min.js"></script>
