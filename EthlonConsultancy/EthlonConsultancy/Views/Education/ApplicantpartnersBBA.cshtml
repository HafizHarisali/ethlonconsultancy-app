
@{
    Layout = "~/Views/Shared/Front.cshtml";
}


<section class="body">

    <!-- start: header -->
    <!-- end: header -->
    <!-- start: sidebar -->
    <!-- end: sidebar -->

    <section role="main" class="">


        <!-- start: page -->


        <section class="panel">
            <header class="panel-heading">
                <div class="panel-actions">
                    <a href="/Admin/#" class="fa fa-caret-down"></a>

                </div>

                <h3 style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;color:black;">Experienced Professionals BA/MBA CVs</h3>
            </header>
            <div class="panel-body">

                <table class="js-dynamitable table table-bordered table-striped mb-none" id="datatable-tabletools">

                    <!-- table heading -->
                    <thead>

                        <tr>
                            <th>Serial #</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Date Of Birth</th>
                            <th>Qualification</th>
                            <th>Qualification Status</th>
                            <th>Industry</th>
                            <th>Training Firm Name</th>

                            <th>Experties</th>
                            <th>Professional Experience</th>
                            <th>Current Org Name</th>
                            <th>Join Date</th>
                            <th>CV</th>

                        </tr>
                        <tr id="sorting">
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th>
                                <input class="js-filter  form-control" type="text" value="">
                            </th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>

                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th></th>

                        </tr>
                    </thead>

                    <!-- table body -->
                    <tbody>
                        @{
                            var count1 = 0;

                            foreach (var item in ViewBag.getappprtbba)
                            {
                                if (item.GName != null && item.GStatus != null)
                                {
                                    count1 = count1 + 1;
                                    <tr class="odd gradeX">
                                        <td>@count1</td>
                                        <td>@item.Name</td>
                                        <td>@item.Email</td>
                                        <td>@item.DOB</td>
                                        <td>@item.GName</td>
                                        <td>@item.GStatus</td>
                                        <td>@item.Industry</td>
                                        <td>@item.FirmName</td>

                                        <td>@item.Experties</td>
                                        <td>@item.TotalExp</td>
                                        <td style="width:80px;">@item.JoinDate</td>
                                        <td style="width:80px;">@item.LeaveDateOPW</td>
                                        <td><a class="btn btn-default" title="Download CV" href="@Url.Action("DownloadApplicantCV", "Applicantinfo", new { item.Id })"><i class="fa fa-download"></i></a></td>

                                    </tr>
                                }

                            }
                        }
                    </tbody>
                </table>


            </div>
            <!-- /.panel-body -->
        </section>
    </section>
</section>



<script src="~/scripts/jquery-3.3.1.min.js"></script>
<!-- dynamitable -->
<script src="~/scripts/dynamitable.jquery.min.js"></script>
