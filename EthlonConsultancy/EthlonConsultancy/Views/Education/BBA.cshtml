
@{
    Layout = "~/Views/Shared/Front.cshtml";
}


<section class="body">

    <!-- start: header -->
    <!-- end: header -->
    <!-- start: sidebar -->
    <!-- end: sidebar -->

    <section role="main" class="">


        <!-- start: page -->


        <section class="panel">
            <header class="panel-heading">
                <div class="panel-actions">
                    <a href="/Admin/#" class="fa fa-caret-down"></a>

                </div>

                <h3 style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;color:black;">Organizations BBA/MBA CVs</h3>
            </header>
            <div class="panel-body">
                <table class="js-dynamitable table table-bordered table-striped mb-none" id="datatable-tabletools">

                    <!-- table heading -->
                    <thead>

                        <tr>
                            <th>Serial #</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Date Of Birth</th>
                            <th>Qualification</th>
                            <th>GPA</th>
                            <th>University</th>
                            <th>Major Subjects</th>
                            <th>Other Subjects</th>
                            <th>Education Board</th>
                            <th>Experience</th>
                            <th>Experience Time</th>
                            <th>Organization Name</th>
                            <th>CV</th>

                        </tr>
                        <tr id="sorting">
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="date" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th>
                                <select class="js-filter  form-control">
                                    <option></option>
                                    <option value="Yes">Yes</option>
                                    <option value="No">No</option>
                                </select>
                            </th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th></th>

                        </tr>
                    </thead>

                    <!-- table body -->
                    <tbody>
                        @{
                            var count1 = 0;

                            foreach (var item in ViewBag.getbbacv)
                            {

                                if (item.GPA != null && item.University != null && item.MajorSubject != null)
                                {
                                    count1 = count1 + 1;
                                    <tr class="">
                                        <td>@count1</td>
                                        <td>@item.Name</td>
                                        <td>@item.Email</td>
                                        <td>@item.DOB</td>
                                        <td>@item.GName</td>
                                        <td>@item.GPA</td>
                                        <td>@item.University</td>
                                        <td>@item.MajorSubject</td>
                                        <td>@item.OtherSubject</td>
                                        <td>@item.EducationBoard</td>
                                        <td>@item.ExpStatus</td>
                                        <td>@item.NOYMExperience</td>
                                        <td>@item.OrgName</td>
                                        <td><a class="btn btn-default" title="Download CV" href="@Url.Action("DownloadFile", "Orginfo", new { item.Id })"><i class="fa fa-download"></i></a></td>

                                    </tr>
                                }
                            }
                        }
                    </tbody>
                </table>

            </div>
        </section>


        <!-- end: page -->
    </section>



</section>

<!-- Vendor -->


<script src="~/scripts/jquery-3.3.1.min.js"></script>
