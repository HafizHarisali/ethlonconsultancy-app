
@{
    ViewBag.Title = "ApplicantCA";
    Layout = "~/Views/Shared/Front.cshtml";
}

<section class="body">

    <!-- start: header -->
    <!-- end: header -->
    <!-- start: sidebar -->
    <!-- end: sidebar -->

    <section role="main" class="">


        <!-- start: page -->


        <section class="panel">
            <header class="panel-heading">
                <div class="panel-actions">
                    <a href="/Admin/#" class="fa fa-caret-down"></a>

                </div>

                <h3 style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;color:black;">Applicant CA/ACCA CVs</h3>
            </header>
            <div class="panel-body">
                <table class="js-dynamitable table table-bordered table-striped mb-none" id="datatable-tabletools">

                    <!-- table heading -->
                    <thead>

                        <tr>
                            <th>Serial #</th>
                            <th>Name</th>
                            <th>Age</th>
                            <th>School</th>
                            <th>Secondary Qualification</th>
                            <th>Higher Qualification</th>
                            <th>Qualification</th>
                            <th>Qualification Status</th>
                            <th>Date Of Qualification / No. Of Papers Left</th>
                            <th>Experience</th>
                            <th>Experience Years</th>
                            <th>Organization</th>
                            <th>CV</th>

                        </tr>
                        <tr id="sorting">
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            <th><input class="js-filter  form-control" type="text" value=""></th>
                            
                            <th></th>

                        </tr>
                    </thead>

                    <!-- table body -->
                    <tbody>
                        @{
                            var count1 = 0;

                            foreach (var item in ViewBag.getcacv)
                            {
                                if (item.Qualname != null && item.Qualstatus != null)
                                {
                                    count1 = count1 + 1;
                                    <tr class="odd gradeX">
                                        <td>@count1</td>
                                        <td>@item.Name</td>
                                        <td>@item.Age</td>
                                        <td>@item.Schoolname</td>
                                        <td>@item.AcadQual</td>
                                        <td>@item.AdvAcadQual</td>
                                        <td>@item.Qualname</td>
                                        <td>@item.Qualstatus</td>
                                        @if (item.Qualstatus == "Qualified")
                                        {
                                            <td>@item.DOQ</td>
                                        }
                                        else
                                        {
                                            <td>@item.NOPL</td>
                                        }
                                        <td>@item.ExpStatus</td>
                                        <td>@item.NOYExperience</td>
                                        <td>@item.OrgName</td>
                                        <td><a class="btn btn-default" title="Download CV" href="@Url.Action("DownloadFile", "Applicantinfo", new { item.Id })"><i class="fa fa-download"></i></a></td>
                                    </tr>
                                }

                            }
                        }
                    </tbody>
                </table>


            </div>
            <!-- /.panel-body -->
        </section>
    </section>
</section>



<script src="~/scripts/jquery-3.3.1.min.js"></script>
<!-- dynamitable -->
<script src="~/scripts/dynamitable.jquery.min.js"></script>
