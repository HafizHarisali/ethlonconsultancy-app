

@{
    Layout = null;
}

<!doctype html>
<html class="fixed">
<head>

    <!-- Basic -->
    <meta charset="UTF-8">
    <title>Organizations | Signup</title>
    <meta name="keywords" content="Ethlon Consultancy" />
    <meta name="description" content="Ethlon Consultancy">
    <meta name="author" content="">
    <link href="~/Admin/assets/images/ethlon-08.png" rel="icon">
    <link href="~/Admin/assets/images/ethlon-08.png" rel="apple-touch-icon">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="/Admin/http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="/Admin/assets/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="/Admin/assets/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="/Admin/assets/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="/Admin/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="/Admin/assets/stylesheets/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="/Admin/assets/stylesheets/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="/Admin/assets/stylesheets/theme-custom.css">
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <!-- Head Libs -->
    <script src="/Admin/assets/vendor/modernizr/modernizr.js"></script>
    <style>
        .body-sign .panel-sign .panel-title-sign .title {
            background-color: #f67d48;
        }

        .body-sign .panel-sign .panel-body {
            border-top-color: #f67d48;
        }

        .btnsubmit {
            background-color: #f67d48;
            color: white;
        }


        body {
            background-image: url(/Admin/assets/images/Consulting.jpg);
            background-size: cover;
            background-attachment:fixed;
        }
    </style>
</head>
<body>
    <!-- start: page -->
    <section class="body-sign">
        <div class="center-sign">
            <a href="@Url.Action("Landing","Home")" class="logo pull-left">
                <img src="~/Admin/assets/images/ethlon-08.png" height="60" alt="Ethlon Consultancy" />
            </a>

            <div class="panel panel-sign">
                <div class="panel-title-sign mt-xl text-right">
                    <h2 class="title text-uppercase text-bold m-none"><i class="fa fa-user mr-xs"></i>Organizations Sign Up</h2>
                </div>
                <div class="panel-body">
                    @using (Html.BeginForm("OrganizationsSignup", "Accounts", FormMethod.Post))
                    {
                        <form>

                            @*@if (TempData["Checkmail"] != null)
                            {
                                <div class="form-group mb-lg">
                                    <p class="form-control input-lg">@TempData["Checkmail"]</p>
                                </div>

                            }*@

                            @*@if (TempData["statusmessage"] != null)
                            {
                                <div class="form-group mb-lg">
                                    <p class="form-control input-lg">@TempData["statusmessage"]</p>
                                </div>

                            }*@

                            <div class="form-group mb-lg">
                                <label>Name</label>
                                <input name="name" type="text" class="form-control input-lg" required/>
                            </div>

                            <div class="form-group mb-none">
                                <div class="row">
                                    <div class="col-sm-12 mb-lg">
                                        <label>Email</label>
                                        <input name="email" type="text" class="form-control input-lg" required/>
                                    </div>
                                    <div class="col-sm-12 mb-lg">
                                        <label>Password</label>
                                        <input name="password" type="password" class="form-control input-lg" required/>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group mb-lg">
                                <label>Coordinator Name</label>
                                <input name="coordinatorname" type="text" class="form-control input-lg" required/>
                            </div>

                            <div class="form-group mb-lg">
                                <label>Coordinator Phone</label>
                                <input name="coordinatorphone" type="number" class="form-control input-lg" required/>
                            </div>

                            <div class="form-group mb-lg">
                                <label>Industry Nature</label>
                                <input name="industrynature" type="text" class="form-control input-lg" required/>
                            </div>
                            
                                <input name="wallet" type="hidden" class="form-control input-lg" />

                           
                            <input name="statusid" type="hidden" class="form-control input-lg" />


                            <div class="row">
                                <div class="col-sm-8">
                                    <div class="">

                                        <label>Already have account?<a href="@Url.Action("OrganizationsLogin","Accounts")">Login</a></label>
                                    </div>
                                    </div>
                                <div class="col-sm-12 text-right">
                                    <button type="submit" class="btn btnsubmit">Sign Up</button>
                                    
                                </div>
                            </div>

                            @*<span class="mt-lg mb-lg line-thru text-center text-uppercase">
                                    <span>or</span>
                                </span>

                                <div class="mb-xs text-center">
                                    <a class="btn btn-facebook mb-md ml-xs mr-xs">Connect with <i class="fa fa-facebook"></i></a>
                                    <a class="btn btn-twitter mb-md ml-xs mr-xs">Connect with <i class="fa fa-twitter"></i></a>
                                </div>

                                <p class="text-center">Already have an account? <a href="/Admin/pages-signin.html">Sign In!</a>*@

                        </form>
                    }
                </div>
            </div>


        </div>
    </section>
    <!-- end: page -->
    <!-- Vendor -->
    <script src="/Admin/assets/vendor/jquery/jquery.js"></script>
    <script src="/Admin/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="/Admin/assets/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="/Admin/assets/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="/Admin/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="/Admin/assets/vendor/magnific-popup/magnific-popup.js"></script>
    <script src="/Admin/assets/vendor/jquery-placeholder/jquery.placeholder.js"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="/Admin/assets/javascripts/theme.js"></script>

    <!-- Theme Custom -->
    <script src="/Admin/assets/javascripts/theme.custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="/Admin/assets/javascripts/theme.init.js"></script>
    <script src="~/scripts/toastr.min.js"></script>
    <script>
    $(function () {
    var a = '@TempData["Checkmail"]';
    if (a != '')
    toastr.error("", a);
    //alert("ab" + a)
    window.location = '#2249506';
    })


    $(function () {
    var a = '@TempData["statusmessage"]';
    if (a != '')
    toastr.success("", a);
    //alert("ab" + a)
    window.location = '#2249506';
    })
    </script>
</body>
</html>

